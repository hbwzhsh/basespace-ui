> csum<-colSums(data)
> rsum<-rowSums(data)
> noiselevel<-as.numeric(Args[3]) #nRows*min(rsum)/length(csum)
> minPval=0.5
> minIndex=0
> maxFC=1
> minFDR=0
> confidence=1-0.05/length(csum)
> for(i in 1:(length(csum)-1))
+ {
+ x=sum(csum[1:i])
+ n=sum(csum)
+ p=i/length(csum)
+ FC=(n-x)*p/(x*(1-p))
+ FDR=noiselevel/x
+ print(FDR)
+ pvalue=prop.test(x,n,p=p,alternative="less",conf.level=confidence)$conf.int[2]
+ if( FDR >0.2)
+ {
+   maxFC=FC
+   minFDR=FDR
+   minIndex=i
+   minPval=pvalue
+ }
+ }
